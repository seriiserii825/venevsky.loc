{
  "extends": "stylelint-config-recommended",
  "plugins": [
    "stylelint-order",
  ],
  "rules": {
    "order/order": [
      "custom-properties",
      "declarations"
  ],
  "order/properties-alphabetical-order": null,
  "at-rule-no-unknown": [true, { "ignoreAtRules": ["function", "if", "return", "else", "each", "include", "mixin"] }] } }
